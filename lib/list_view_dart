

ListView.builder(
              padding: const EdgeInsets.all(30),
              itemCount: globals.topTen.length,
              itemBuilder: (BuildContext context, int index) {
                return Container(
                  height: 50,
                  color: Colors.grey,
                  child: Center(
                    child: Text('${globals.topTen[index]}'),
                  ),
                );
              }
            )